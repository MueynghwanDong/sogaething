type User {
    id : ID!
    name : String!
    email : String!
    imageUrl : String!
    emailVerified : Boolean
    provider : String!
    providerId : String!
    phone : String!
    address : String!
    trust : Int!
    token : String!
}

type LoginOutput {
    output : String!
}

input LoginUserInput {
    provider : String!
}

extend type Mutation {
    loginUser(input: LoginUserInput!): LoginOutput
}
