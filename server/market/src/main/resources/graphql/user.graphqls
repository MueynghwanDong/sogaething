type User {
    id : ID!
    name : String!
    email : String
    imageUrl : String!
    provider : String!
    providerId : Int!
    phone : String!
    address : String!
    trust : Int!
}

type LoginOutput {
    token : String!
}

input LoginUserInput {
    provider : String!
}

extend type Query {
    findAllUsers: [User]
    loginUser: LoginOutput
}