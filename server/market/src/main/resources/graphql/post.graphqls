type Post {
    postId : ID!
    user : User!
    isBuy : Boolean!
    title : String!
    saleDate : String!
    contents : String!
    viewCount : Int!
    deal : String!
}

type Query {
	findAllPosts: [Post]
}

input CreatePostInput {
    uploaderId : Int!
    title : String!
    saleDate : String!
    contents : String!
    deal : String!
}

type Mutation {
    createPost(input: CreatePostInput!): Post
}