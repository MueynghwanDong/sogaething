type Matching {
    matchingId: ID
    user: User
    category: String
    hashtag: String
    minPrice: Int
    maxPrice: Int
    transaction: String
    createdDate: String
    modifiedDate: String
}

extend type Query {
    findMatchingByUserId: [Matching]
}

extend type Mutation {
    createMatching(input: MatchInput!): Matching
}